{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/public/*": ["./public/*"],
      "@/libs/*": ["./src/app/libs/*"],
      "@/auth/*": ["./src/api/auth/*"],
      "@/api/*": ["./src/api/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "tailwind.config.js", "src/app/api/auth/[...nextauth]/route.js", "src/app/page.jsx", "src/components/navBar.jsx", "src/app/api/auth/layout.jsx"],
  "exclude": ["node_modules"]
}
